<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basictodo">

    <application
        android:name=".ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/tick_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tick_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BasicTODO">
        <activity android:name=".TaskActivity"></activity>
        <activity android:name=".SetDate" />
        <activity android:name=".TODODataActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>